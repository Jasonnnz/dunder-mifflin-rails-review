<h1><%= @dog.name %></h1>
<p>Breed: <%= @dog.breed %></p>
<p>Age: <%= @dog.age %></p>

<h4>List of Employees:</h4>
<ul>
<% @dog.employees.each do |employee| %>
    <li><%= link_to (employee.first_name + " " + employee.last_name), employee_path(employee)%></li>
<% end %>
</ul>

<%= button_to "Delete Dog", dog_path(@dog), method: :delete %>